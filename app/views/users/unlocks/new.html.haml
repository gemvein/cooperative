- add_title 'Resend unlock instructions'
= bootstrap_form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post, :class => 'form form-horizontal'}) do |f|
  = f.email_field :email, :autofocus => true
  .controls= f.submit "Resend unlock instructions"
= render "user/shared/links"
